<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#101820</Color>
    <Color x:Key="ForegroundColor">#FFB612</Color>
    <SolidColorBrush x:Key="WhiteBrush"
                     Color="White" />
    <SolidColorBrush x:Key="HintTextBrush"
                     Color="#666666" />
    <SolidColorBrush x:Key="ForegroundBrush"
                     Color="#FFB612"/>
    <SolidColorBrush x:Key="BackgroundBrush"
                     Color="#101820"/>
    <SolidColorBrush x:Key="DisabledBrush"
                     Color="Gray" />

    <SolidColorBrush x:Key="ToolTipBackground"
                     Color="#A5ACAF"/>

    <SolidColorBrush x:Key="ToolTipForeground"
                     Color="#101820"/>
</ResourceDictionary>